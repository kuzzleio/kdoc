{
  "name": "kuzdoc",
  "description": "The CLI that helps build the Kuzzle Docs",
  "version": "1.1.5",
  "author": "Luca Marchesini @xbill82",
  "bin": {
    "kuzdoc": "./bin/run"
  },
  "bugs": "https://github.com/kuzzleio/kuzzle-documentation-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3",
    "@types/listr": "^0.14.2",
    "axios": "^0.19.0",
    "cli-ux": "^5.4.1",
    "execa": "^4.0.0",
    "express": "^4.17.1",
    "listr": "^0.14.3",
    "tslib": "^1.10.0",
    "yaml": "^1.7.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/express": "^4.17.2",
    "@types/node": "^10.17.13",
    "@types/yaml": "^1.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kuzzleio/kuzzle-documentation-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "kuzdoc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kuzzleio/kuzzle-documentation-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
